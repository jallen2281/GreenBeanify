#!/usr/bin/env node
var debug = require('debug')('GreenBeanify');
var app = require('../app');
var greenBean = require('../greenBean');
config = require('../config');

app.set('port', process.env.PORT || config.listen_port);

var server = app.listen(app.get('port'), function() {
	console.log('GreenBeanify server started on port ' + server.address().port);
});
